println "Defining a t1 range...";
range rtype1 "a" = "5".."10";

println "Implementing a t1 range...";
rangev rtype1 "a" "a" = "6";


println "Testing load and store...";
print "a: ";
load range rtype1 "a";
println;

load aload 10;
store range rtype1 "a";

print "a: ";
load range rtype1 "a";
println;


println "Quitting...";
end 0;