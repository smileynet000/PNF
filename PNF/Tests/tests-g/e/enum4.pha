println "Defining an enum...";

enum a = 
{
 "a",
 "b",
 "c"
};

println "Implementing an enum...";
enumv "a" "a" = "c";


println "Testing load and store...";
print "enum a: ";
load venum "a";
println;

load aload "b";
store venum "a";

print "enum a: ";
load venum "a";
println;

println "Quitting...";
end 0;